<%= form_with(model: song_library, local: true) do |form| %>
  <% if song_library.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(song_library.errors.count, "error") %> prohibited this song_library from being saved:</h2>

      <ul>
        <% song_library.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :ID %>
    <%= form.number_field :Song_id%>
  </div>

  <div class="field">
    <%= form.label :Title %>
    <%= form.text_field :Title %>
  </div>

  <div class="field">
    <%= form.label :Artist %>
    <%= form.text_field :Artist %>
  </div>

  <div class="field">
    <%= form.label :Release_Date %>
    <%= form.date_select :Release_Date %>
  </div>

  <div class="field">
    <%= form.label :Price_in_Doller %>
    <%= form.number_field :Price %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
